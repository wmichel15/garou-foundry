      }
    },
    {
      "sort": 400000,
      "name": "IV. Accept an Honorable Surrender",
      "type": "text",
      "_id": "2R1EPF33DVE2UXXE",
      "system": {},
      "title": {
        "show": true,
        "level": 1
      },
      "image": {},
      "text": {
        "format": 1,
        "content": "<p>Victory ends when a foe yields, and mercy shown to one of their own preserves the strength and honor of the Garou.</p>"
      },
      "video": {
        "controls": true,
        "volume": 0.5
      },
      "src": null,
      "category": null,
      "flags": {},
      "_stats": {
        "compendiumSource": null,
