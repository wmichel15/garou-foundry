      "system": {},
      "title": {
        "show": true,
        "level": 1
      },
      "image": {},
      "text": {
        "format": 1,
        "content": "<p>Leadership must be tested openly in times of peace to prevent weakness, stagnation, or tyranny.</p>"
      },
      "video": {
        "controls": true,
