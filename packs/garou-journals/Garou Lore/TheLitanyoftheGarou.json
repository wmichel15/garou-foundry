      "system": {},
      "title": {
        "show": true,
        "level": 1
      },
      "image": {},
      "text": {
        "format": 1,
        "content": "<p>True honor lies in showing restraint and respect to the weak, lest the Garou become the monsters they fear.</p>"
      },
      "video": {
        "controls": true,
        "volume": 0.5
      },
      "src": null,
