      }
    },
    {
      "sort": 300000,
      "name": "III. Respect the Territory of Another",
      "type": "text",
      "_id": "MjTvj4PgEWGoCQag",
      "system": {},
      "title": {
        "show": true,
        "level": 1
      },
      "image": {},
      "text": {
        "format": 1,
        "content": "<p>Territory must be honored and entered with respect, as ignoring boundaries invites conflict and fractures Garou unity.</p>"
      },
      "video": {
        "controls": true,
        "volume": 0.5
      },
      "src": null,
      "category": null,
      "flags": {},
      "_stats": {
