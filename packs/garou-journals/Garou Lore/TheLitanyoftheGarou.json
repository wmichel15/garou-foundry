      "_id": "Chha9Wr9mqsH8XDI",
      "system": {},
      "title": {
        "show": true,
        "level": 1
      },
      "image": {},
      "text": {
        "format": 1,
        "content": "<p>The Garou must remain unseen among humanity, for revelation brings panic, persecution, and devastation.</p><p data-start=\"3343\" data-end=\"3517\"></p>"
      },
      "video": {
        "controls": true,
        "volume": 0.5
